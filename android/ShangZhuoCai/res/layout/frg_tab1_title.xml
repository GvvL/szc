<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/nav_bar_hight"
    android:background="@color/black"
    android:paddingStart="@dimen/pad_lv7"
    android:paddingEnd="@dimen/pad_lv7" >
    <TextView 
        android:id="@+id/nar_bar_address"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textColor="@color/white"
        android:text="@string/str_tab1_address"
        android:textStyle="bold"
        android:textSize="@dimen/font_title"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:drawableRight="@drawable/arrow_down"
        />
    <ImageButton 
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:src="@drawable/ic_logo1"
        android:background="@null"
        android:layout_centerInParent="true"/>
    <TextView 
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textColor="@color/white"
        android:text="●●●"
        android:textSize="@dimen/font_little"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:visibility="gone"/>
    

</RelativeLayout>
